				<artifactId>experian-srv-integracao-kta-client</artifactId>
				<version>${srv-integracao-kta-client-version}</version>
				<exclusions>
					<exclusion>
						<groupId>br.com.experian</groupId>
						<artifactId>experian-commons-jax-ws-2.1.7</artifactId>
