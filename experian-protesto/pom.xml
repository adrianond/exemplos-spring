	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<version>2.5</version>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>scm:svn:http://spobrccm01-pi/svn/SistemasCSeDAeMS/Desenvolvimento/SistemasCS/AplicacoesWeb/ProtestoWeb/branches/desenv/experian-protesto</connection>
	</scm>

