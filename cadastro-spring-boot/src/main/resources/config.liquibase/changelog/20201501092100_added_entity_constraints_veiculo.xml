<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Veiculo.
    -->
    <changeSet id="20201501092100-2" author="develop">

        <addForeignKeyConstraint baseColumnNames="cod_proprietario"
                                 baseTableName="veiculo"
                                 constraintName="fk_veiculo_proprietario"
                                 referencedColumnNames="codigo"
                                 referencedTableName="proprietario"/>

        <addForeignKeyConstraint baseColumnNames="id_acessorio"
                                 baseTableName="veiculo"
                                 constraintName="fk_veiculo_acessorio"
                                 referencedColumnNames="codigo"
                                 referencedTableName="acessorio"/>


    </changeSet>
</databaseChangeLog>
