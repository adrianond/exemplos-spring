<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20201501092100-2" author="develop">

        <addForeignKeyConstraint baseColumnNames="codigo_endereco"
                                 baseTableName="empresa"
                                 constraintName="fk_empresa_endereco"
                                 referencedColumnNames="codigo"
                                 referencedTableName="endereco_empresa"/>

        <addForeignKeyConstraint baseColumnNames="id_telefone"
                                 baseTableName="empresa"
                                 constraintName="fk_empresa_telefone"
                                 referencedColumnNames="codigo"
                                 referencedTableName="telefone"/>


    </changeSet>
</databaseChangeLog>
