<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>


    <!--
        Added the entity Veiculo.
    -->
    <changeSet id="20201501092100-1" author="develop">
        <createTable tableName="veiculo">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="ano_modelo" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="ano_fabricacao" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="valor_cotacao" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="cod_proprietario" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="id_acessorio" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="razao_social" type="varchar(100)">
                <constraints nullable="false"/>
            </column>

            <column name="cnpj" type="varchar(15)">
                <constraints nullable="false"/>
            </column>

            <column name="email" type="varchar(60)">
                <constraints nullable="false"/>
            </column>

        </createTable>

        <createSequence sequenceName="SEQ_VEICULO"/>

    </changeSet>
</databaseChangeLog>